{
  "name": "@aryan02420/vue-frame",
  "description": "Render your Vue nodes in an iframe",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/vue-frame.js",
  "main": "./dist/vue-frame.umd.cjs",
  "types": "./dist/vue-frame.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-frame.js",
      "require": "./dist/vue-frame.umd.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:demo": "vue-tsc --noEmit && vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx,vue}"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.6.5",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^0.40.4"
  },
  "author": "aryan02420 <aryan02420@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryan02420/vue-frame.git"
  },
  "keywords": [
    "vue",
    "iframe"
  ],
  "bugs": {
    "url": "https://github.com/aryan02420/vue-frame/issues"
  },
  "homepage": "https://github.com/aryan02420/vue-frame#readme"
}
